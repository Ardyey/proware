<!DOCTYPE html>
<html lang="en">

<head>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <title>Proware Admin</title>
  <!-- plugins:css -->
  <link rel="stylesheet" href="vendors/iconfonts/mdi/css/materialdesignicons.min.css">
  <link rel="stylesheet" href="vendors/css/vendor.bundle.base.css">
  <!-- endinject -->
  <!-- inject:css -->
  <link rel="stylesheet" href="css/style.css">
  <link rel="stylesheet" href="css/custom.css">
  <link rel="stylesheet" href="css/dashboard.css">
  <!-- endinject -->
  <link rel="shortcut icon" id="favicon" href="images/Owner.png" />
</head>

<body>
  <div class="container-scroller">
    <p id="position" style="display:none"><%= position %></p>
    <!-- partial:partials/_navbar.html -->
    <%- include('../../partials/_navbar.html')%>
    <!-- partial -->
    <div class="container-fluid page-body-wrapper">
      <!-- partial:partials/_sidebar.html -->
      <%- include('../../partials/_sidebar.ejs')%>
      <!-- partial -->
      <div class="main-panel">
        <div class="content-wrapper">
          <div class="row">
          </div>
          <div class="page-header">
            <h3 class="page-title">
              <span class="page-title-icon bg-gradient-primary text-white mr-2">
                <i class="mdi mdi-home"></i>
              </span>
              Dashboard
            </h3>
            <nav aria-label="breadcrumb">
              <ul class="breadcrumb">
                <li class="breadcrumb-item active" aria-current="page">
                  <%- include('../../partials/_time.html')%>
                  <i class="mdi mdi-alert-circle-outline icon-sm text-primary align-middle"></i>
                </li>
              </ul>
            </nav>
          </div>
          <div class="row">
            <%if(position === 'Admin' || position === 'PAMO' || position === 'PROWARE') {%>
            <div class="col-md-4 stretch-card grid-margin">
              <div class="card bg-gradient-danger card-img-holder text-white shadow sm rounded" id="pendingOrders">
                <div class="card-body">
                  <img src="images/dashboard/circle.svg" class="card-img-absolute" alt="circle-image" />
                  <h4 class="font-weight-normal mb-3">Pending Orders
                    <i class="mdi mdi-chart-line mdi-24px float-right"></i>
                  </h4>
                  <h2 class="mb-5" id="dPending"></h2>
                </div>
              </div>
            </div>
            <%}%>
            <%if(position === 'Admin' || position === 'CASHIER') {%>
            <div class="col-md-4 stretch-card grid-margin">
              <div class="card bg-gradient-success card-img-holder text-white shadow sm rounded" id="approvedOrders">
                <div class="card-body">
                  <img src="images/dashboard/circle.svg" class="card-img-absolute" alt="circle-image" />
                  <h4 class="font-weight-normal mb-3">Approved Orders
                    <i class="mdi mdi-check-all mdi-24px float-right"></i>
                  </h4>
                  <h2 class="mb-5" id="approvedOrder"></h2>
                  <h5 class="card-text">Awaiting Payment</h5>
                </div>
              </div>
            </div>
            <%}%>
            <%if(position === 'Admin' || position === 'PAMO' || position === 'PROWARE') {%>
            <div class="col-md-4 stretch-card grid-margin">
              <div class="card bg-gradient-danger card-img-holder text-white shadow sm rounded" id="paidOrders">
                <div class="card-body">
                  <img src="images/dashboard/circle.svg" class="card-img-absolute" alt="circle-image" />
                  <h4 class="font-weight-normal mb-3">Paid Orders
                    <i class="mdi mdi-cash-multiple mdi-24px float-right"></i>
                  </h4>
                  <h2 class="mb-5" id="paidOrder"></h2>
                </div>
              </div>
            </div>
            <%}%>
            <%if(position === 'Admin' || position === 'PAMO' || position === 'PROWARE') {%>
            <div class="col-md-4 stretch-card grid-margin">
              <div class="card bg-gradient-info card-img-holder text-white shadow sm rounded" id="successfulOrders">
                <div class="card-body">
                  <img src="images/dashboard/circle.svg" class="card-img-absolute" alt="circle-image" />
                  <h4 class="font-weight-normal mb-3">Successful Orders
                    <i class="mdi mdi-bookmark-check mdi-24px float-right"></i>
                  </h4>
                  <h2 class="mb-5" id="dailyOrder"></h2>
                  <h5 class="dateToday card-text"></h5>
                </div>
              </div>
            </div>
            <%}%>
            <%if(position === 'Admin' || position === 'PAMO' || position === 'PROWARE') {%>
            <div class="col-md-4 stretch-card grid-margin">
              <div class="card bg-gradient-danger card-img-holder text-white  shadow sm rounded" id="cancelledOrders">
                <div class="card-body">
                  <img src="images/dashboard/circle.svg" class="card-img-absolute" alt="circle-image" />
                  <h4 class="font-weight-normal mb-3">Cancelled Orders
                    <i class="mdi mdi-bookmark-remove mdi-24px float-right"></i>
                  </h4>
                  <h2 class="mb-5" id="cancelledOrder"></h2>
                  <h5 class="dateToday card-text"></h5>
                </div>
              </div>
            </div>
            <%}%>
            <%if(position === 'Admin' || position === 'CASHIER') {%>
            <div class="col-md-4 stretch-card grid-margin">
              <div class="card bg-gradient-success card-img-holder text-white  shadow sm rounded" id="exchangeOrdersP">
                <div class="card-body">
                  <img src="images/dashboard/circle.svg" class="card-img-absolute" alt="circle-image" />
                  <h4 class="font-weight-normal mb-3">Exchange Item Pending
                    <i class="mdi mdi-swap-horizontal mdi-24px float-right"></i>
                  </h4>
                  <h2 class="mb-5" id="exchangeOrderP"></h2>
                </div>
              </div>
            </div>
            <%}%>
            <%if(position === 'Admin' || position === 'PAMO' || position === 'PROWARE') {%>
            <div class="col-md-4 stretch-card grid-margin">
              <div class="card bg-gradient-success card-img-holder text-white  shadow sm rounded" id="exchangeOrders">
                <div class="card-body">
                  <img src="images/dashboard/circle.svg" class="card-img-absolute" alt="circle-image" />
                  <h4 class="font-weight-normal mb-3">Exchange Item Orders
                    <i class="mdi mdi-swap-horizontal mdi-24px float-right"></i>
                  </h4>
                  <h2 class="mb-5" id="exchangeOrder"></h2>
                  <h5 class="dateToday card-text"></h5>
                </div>
              </div>
            </div>
            <%}%>
            <%if(position === 'Admin' || position === 'PAMO' || position === 'PROWARE' || position === 'CASHIER') {%>
            <div class="col-md-4 stretch-card grid-margin">
              <div class="card bg-gradient-info card-img-holder text-white  shadow sm rounded" id="expiredOrders">
                <div class="card-body">
                  <img src="images/dashboard/circle.svg" class="card-img-absolute" alt="circle-image" />
                  <h4 class="font-weight-normal mb-3">Expired Orders
                    <i class="mdi mdi-swap-horizontal mdi-24px float-right"></i>
                  </h4>
                  <h2 class="mb-5" id="expiredOrder"></h2>
                  <h5 class="dateToday card-text"></h5>
                </div>
              </div>
            </div>
            <%}%>
          </div>
          <!---->
        </div>
        <!-- content-wrapper ends -->
        <!-- partial:partials/_footer.html -->
        <%- include('../../partials/_footer.html')%>
            <!-- partial -->
          </div>
          <!-- main-panel ends -->
        </div>
        <!-- page-body-wrapper ends -->
      </div>
      <!-- container-scroller -->

      <!-- plugins:js -->
      <script src="js/time.js"></script>
      <script src="vendors/js/vendor.bundle.base.js"></script>
      <script src="vendors/js/vendor.bundle.addons.js"></script>
      <script src="/socket.io/socket.io.js"></script>
      <!-- endinject -->
      <!-- Plugin js for this page-->
      <!-- End plugin js for this page-->
      <!-- inject:js -->
      <script src="js/off-canvas.js"></script>
      <script src="js/misc.js"></script>
      <!-- endinject -->
      <!-- Custom js for this page-->
      <script src="js/dashboard.js"></script>
      <!-- End custom js for this page-->
      <script src="js/home/dashboard.js"></script>
</body>

</html>